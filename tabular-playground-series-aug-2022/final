    "imp_mean = SimpleImputer( strategy='most_frequent') #'median'을 쓰면 중앙값사용\n",
    "imp_mean.fit(freq_null)\n",
    "freq_data_imputed = pd.DataFrame(imp_mean.transform(freq_null))\n",
    "print(freq_data_imputed.isnull().sum())\n",
    "print(freq_data_imputed.shape)"
   ]
